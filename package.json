{
  "name": "sendeliver",
  "version": "1.0.0",
  "scripts": {
    "clean": "node back/src/scripts/clean.js",
    "dev": "concurrently \"cd front && npm run dev\" \"cd back && npm run dev\"",
    "build": "npm run build:logger && npm run build:back && npm run build:front",
    "build:logger": "cd packages/logger && npm runuelva",
    "build:back": "cd back && npm run build",
    "build:front": "cd front && npm run build",
    "prisma:migrate": "prisma migrate dev",
    "prisma:generate": "prisma generate"
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  },
  "workspaces": [
    "packages/*",
    "back",
    "front",
    "prisma"
  ]
}