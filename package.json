{
  "name": "sendeliver-root",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "install": "npm run install:back && npm run install:front",
    "install:front": "cd front && npm install",
    "install:back": "cd back && npm install",
    "dev": "concurrently \"cd front && npm run dev\" \"cd back && npm run dev\"",
    "start": "cd back && npm start",
    "build": "concurrently \"npm run build:front\" \"npm run build:back\"",
    "build:front": "cd front && npm run build",
    "build:back": "cd back && npm run build",
    "clean": "node back/src/scripts/clean.js",
    "depcheck": "npx depcheck"
  },
  "dependencies": {
  "concurrently": "^9.1.0"
    
  }
}
