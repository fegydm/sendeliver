{
  "name": "sendeliver-root",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "clean": "node back/src/scripts/clean.js",
    "install:back": "cd back && npm install",
    "install:front": "cd front && npm install",
    "install": "npm run install:parallel",
    "install:parallel": "npm run install:back & npm run install:front",
    "build:back": "cd back && npm run build",
    "build:front": "cd front && npm run build",
    "build": "npm run build:parallel",
    "build:parallel": "npm run build:back & npm run build:front",
    "dev:back": "cd back && npm run dev",
    "dev:front": "cd front && npm run dev",
    "dev": "npm run dev:parallel",
    "dev:parallel": "npm run dev:back & npm run dev:front",
    "start:back": "cd back && npm start",
    "start:front": "cd front && npm run preview",
    "start": "npm run start:parallel",
    "start:parallel": "npm run start:back & npm run start:front"
  }
}
