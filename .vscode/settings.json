{
  "editor.defaultFormatter": null,
  "editor.formatOnSave": false,
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },

"multiCommand.commands": [
  {
    "command": "multiCommand.findCurrentFileReferences",
    "sequence": [
      "editor.action.copyFileName",
      {
        "command": "workbench.action.findInFiles",
        "args": {
          "triggerSearch": true,
          "matchWholeWord": false,
          "isCaseSensitive": false,
          "isRegexp": false
        }
      },
      "editor.action.clipboardPasteAction"
    ]
  }
],

  "workbench.panel.defaultLocation": "right",
  "workbench.view.remote.panel.visible": true,
  "remote.autoForwardPortsSource": "output",
  "remote.portsAttributes": {
    "3000": {
      "label": "Frontend",
      "onAutoForward": "openBrowser",
      "elevateIfNeeded": true
    },
    "5000": {
      "label": "Backend",
      "onAutoForward": "notify",
      "elevateIfNeeded": true
    }
  },
  "remote.autoForwardPorts": true,
  "remote.forwardOnOpen": true,
  "files.associations": {
    "*.css": "css"
  },
  "cSpell.words": [
    "artic",
    "contenteditable",
    "cooldown",
    "Cpath",
    "Csvg",
    "Čeština",
    "darkmode",
    "Deutsch",
    "dropfilter",
    "ehthumbs",
    "eslintcache",
    "flexbox",
    "grayscale",
    "ILIKE",
    "imask",
    "Košice",
    "liftgate",
    "lightgray",
    "linecap",
    "linejoin",
    "navbars",
    "njsproj",
    "Nominatim",
    "ntvs",
    "openai",
    "preinstall",
    "Rulez",
    "sendeliver",
    "schengen",
    "Slovenčina",
    "Slovensko",
    "tailwindcss",
    "tsoa",
    "vercel",
    "VITE"
  ]
}
