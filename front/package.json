{
  "name": "sendeliver-frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "proxy": "http://localhost:5000",
  "scripts": {
    "dev": "vite --config configs/vite.config.ts",
    "build": "tsc && vite build --config configs/vite.config.ts",
    "preview": "vite preview --config configs/vite.config.ts",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint --config ./configs/eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=true eslint --fix --config ./configs/eslint.config.js \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\" --config configs/.prettierrc",
    "test": "vitest --config configs/vitest.config.ts",
    "test:watch": "vitest watch --config configs/vitest.config.ts",
    "test:coverage": "vitest run --coverage --config configs/vitest.config.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "1.1.2",
    "@radix-ui/react-dropdown-menu": "2.1.2",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-select": "2.1.2",
    "@radix-ui/react-slot": "1.1.0",
    "axios": "1.7.7",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "lottie-web": "5.12.2",
    "lucide-react": "0.454.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-router-dom": "6.27.0",
    "tailwind-merge": "2.5.4",
    "zustand": "5.0.1"
  },
  "devDependencies": {
    "@types/node": "20.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "@vitejs/plugin-react": "4.3.3",
    "autoprefixer": "10.4.20",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.32.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.8",
    "tailwindcss": "3.4.14",
    "typescript": "5.3.3",
    "vite": "4.5.0",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "1.1.4"
  },
  "overrides": {
    "rollup": "npm:@rollup/wasm-node"
  }
}
