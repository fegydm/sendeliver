    "@types/swagger-ui-express": "4.1.7",
    "@types/ws": "8.5.13",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "autoprefixer": "^10.4.20",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.18",
    "prettier": "^3.3.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "10.9.2",
    "tsoa": "6.5.1",
    "typescript": "5.6.3"
