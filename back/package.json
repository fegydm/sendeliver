{
  "name": "sendeliver-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node server.mjs",
    "dev": "node --loader ts-node/esm server.mjs",
    "build": "tsc --module es2022 --target es2022",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "5.7.0",
    "bcryptjs": "2.4.3",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.4.5",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "express-rate-limit": "7.1.5",
    "express-validator": "7.0.1",
    "helmet": "7.1.0",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.2",
    "morgan": "1.10.0",
    "openai": "4.28.0",
    "pg": "8.11.3",
    "redis": "4.6.7",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "winston": "3.11.0",
    "ws": "8.13.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.11",
    "@types/node": "20.9.0",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "27.6.0",
    "jest": "29.7.0",
    "nodemon": "3.0.1",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "typescript": "4.4.4"
  },
  "jest": {
    "config": "./configs/jest.config.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
