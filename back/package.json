    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "4.21.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "glob": "11.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.28.0",
    "pg": "^8.11.3",
    "redis": "^4.6.7",
